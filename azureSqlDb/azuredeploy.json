{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dbName": {
            "type": "string"
        },
        "sqlServerId": {
            "type": "string"
        },
        "sqlServerName": {
            "type": "string"
        },
    "elasticPoolName": {
        "type": "string"
    }
    },
    "variables": {

    },
    "resources": [
        {
            "type": "Microsoft.Sql/servers/databases",
            "name": "[concat(parameters('sqlServerName'),'/',parameters('dbName'))]",
            "apiVersion": "2014-04-01",
            "location": "[resourceGroup().location]",
            "tags": {
                "TENANT": "[parameters('dbName')]"
            },
            "properties": {
                "collation": "SQL_Latin1_General_CP1_CI_AS",
                "elasticPoolName": "[parameters('elasticPoolName')]"

            }
        },
        {
            "name": "[concat(parameters('sqlServerName'), '/', parameters('dbName'), '/current')]",
            "type": "Microsoft.Sql/servers/databases/transparentDataEncryption",
            "apiVersion": "2014-04-01",
            "dependsOn": [
                "[resourceId('Microsoft.Sql/servers/databases', parameters('sqlServerName'), parameters('dbName'))]"
            ],
            "properties": {
              "status": "Enabled"
            }
          }
    ],
    "outputs": {}
}